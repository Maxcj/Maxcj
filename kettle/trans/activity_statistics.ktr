  <step>
    <name>各个社团的活动统计</name>
    <type>TableInput</type>
    <description />
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>本地数据库</connection>
    <sql>SELECT dd.id,
	dd.fullname,
	IFNULL(aa.activity_num, 0) AS commmon_activity_num,
	IFNULL(bb.activity_num, 0) AS great_activity_num,
	now() as update_time
FROM
	(SELECT d.id, d.fullname
		FROM sys_dept AS d
		WHERE d.pid = '38'
		GROUP BY d.id
	) AS dd
LEFT JOIN (
	SELECT a.activity_club,
		COUNT(DISTINCT a.activity_id) AS activity_num
	FROM sys_activity AS a
	WHERE a.activity_state >= '0'
	AND a.activity_category = '1'
	GROUP BY a.activity_club
) AS aa ON dd.id = aa.activity_club
LEFT JOIN (
	SELECT a.activity_club,
		COUNT(DISTINCT a.activity_id) AS activity_num
	FROM sys_activity AS a
	WHERE a.activity_state >= '0'
	AND a.activity_category = '2'
	GROUP BY
		a.activity_club
) AS bb ON dd.id = bb.activity_club</sql>
